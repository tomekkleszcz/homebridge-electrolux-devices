        "strict-uri-encode": "^2.0.0"
    },
    "devDependencies": {
        "@types/node": "^16.10.9",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "homebridge": "^1.6.0",
        "nodemon": "^2.0.20",
        "rimraf": "^3.0.2",
